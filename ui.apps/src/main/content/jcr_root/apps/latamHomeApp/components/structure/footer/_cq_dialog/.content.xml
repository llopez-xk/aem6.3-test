<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <footerLinksTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Footer - List of links"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columnA
                                jcr:primaryType="nt:unstructured"
                                jcr:title="First Column"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <columnTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title"
                                        name="./titleColumnA"/>
                                    <showtitleinmobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Show title in mobile"
                                        name="./showinmobileA"
                                        text="Show title in mobile"/>
                                    <linktitleA
                                        jcr:primaryType="nt:unstructured"
                                        anchor="95%"
                                        defaultAlt="*"
                                        defaultPath="*"
                                        defaultTitle="*"
                                        id="linktitleA"
                                        name="./linktitleA"
                                        pathAllowBlank="{Boolean}false"
                                        textAllowBlank="{Boolean}false"
                                        titleAllowBlank="{Boolean}false"
                                        xtype="latamlink"/>
                                    <fields/>
                                </items>
                            </columnA>
                            <columnB
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Second Column"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <columnTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title"
                                        name="./titleColumnB"/>
                                    <showtitleinmobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Show title in mobile"
                                        name="./showinmobileB"
                                        text="Show title in mobile"/>
                                    <linktitleB
                                        jcr:primaryType="nt:unstructured"
                                        anchor="95%"
                                        defaultAlt="*"
                                        defaultPath="*"
                                        defaultTitle="*"
                                        id="linktitleB"
                                        name="./linktitleB"
                                        pathAllowBlank="{Boolean}false"
                                        textAllowBlank="{Boolean}false"
                                        titleAllowBlank="{Boolean}false"
                                        xtype="latamlink"/>
                                    <fields/>
                                </items>
                            </columnB>
                            <columnC
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Third Column"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <columnTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title"
                                        name="./titleColumnC"/>
                                    <showtitleinmobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Show title in mobile"
                                        name="./showinmobileC"
                                        text="Show title in mobile"/>
                                    <linktitleC
                                        jcr:primaryType="nt:unstructured"
                                        anchor="95%"
                                        defaultAlt="*"
                                        defaultPath="*"
                                        defaultTitle="*"
                                        id="linktitleC"
                                        name="./linktitleC"
                                        pathAllowBlank="{Boolean}false"
                                        textAllowBlank="{Boolean}false"
                                        titleAllowBlank="{Boolean}false"
                                        xtype="latamlink"/>
                                    <fields/>
                                </items>
                            </columnC>
                            <columnD
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Fourth Column"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <columnTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title"
                                        name="./titleColumnD"/>
                                    <showtitleinmobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Show title in mobile"
                                        name="./showinmobileD"
                                        text="Show title in mobile"/>
                                    <linktitleD
                                        jcr:primaryType="nt:unstructured"
                                        anchor="95%"
                                        defaultAlt="*"
                                        defaultPath="*"
                                        defaultTitle="*"
                                        id="linktitleD"
                                        name="./linktitleD"
                                        pathAllowBlank="{Boolean}false"
                                        textAllowBlank="{Boolean}false"
                                        titleAllowBlank="{Boolean}false"
                                        xtype="latamlink"/>
                                    <fields/>
                                </items>
                            </columnD>
                        </items>
                    </column>
                </items>
            </footerLinksTab>
        </items>
    </content>
</jcr:root>
