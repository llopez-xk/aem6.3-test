<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer Global"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <firstColum
                jcr:primaryType="nt:unstructured"
                jcr:title="First Column"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columnTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./titleColumnA"/>
                            <showtitleinmobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldLabel="Show title in mobile"
                                name="./showinmobileA"
                                text="Show title in mobile"
                                value="true"/>
                            <link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                fieldLabel="Link"
                                name="./link"/>
                            <openinnewwindow
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldLabel="Show title in mobile"
                                name="./target"
                                text="Open in a new window"
                                value="true"/>
                            <links
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/collapsible"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/multifield">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            acs-commons-nested="JSON_STORE"
                                            name="./links">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <link
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Path or URL of the page"
                                                            fieldLabel="Link"
                                                            name="./link"/>
                                                        <text
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Text for the link"
                                                            fieldLabel="Text"
                                                            name="./text"/>
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Title for the link tooltip"
                                                            fieldLabel="Title"
                                                            name="./title"/>
                                                        <target
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                            name="./target"
                                                            text="Open in new window"
                                                            value="true"/>
                                                        <openinnewwindow
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                            fieldLabel="Open in"
                                                            name="./openinnewwindow">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <self
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Same Window"
                                                                    value="_self"/>
                                                                <blank
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="New Window"
                                                                    value="_blank"/>
                                                            </items>
                                                        </openinnewwindow>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </links>
                                </items>
                            </links>
                        </items>
                    </column>
                </items>
            </firstColum>
            <secondColum
                jcr:primaryType="nt:unstructured"
                jcr:title="Second Column"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columnTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./titleColumnB"/>
                            <showtitleinmobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldLabel="Show title in mobile"
                                name="./showinmobileB"
                                text="Show title in mobile"/>
                            <linktitleB
                                jcr:primaryType="nt:unstructured"
                                anchor="95%"
                                defaultAlt="*"
                                defaultPath="*"
                                defaultTitle="*"
                                id="linktitleB"
                                name="./linktitleB"
                                pathAllowBlank="{Boolean}false"
                                textAllowBlank="{Boolean}false"
                                titleAllowBlank="{Boolean}false"
                                xtype="latamlink"/>
                            <fields/>
                        </items>
                    </column>
                </items>
            </secondColum>
            <thirdColum
                jcr:primaryType="nt:unstructured"
                jcr:title="Third Column"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columnTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./titleColumnC"/>
                            <showtitleinmobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldLabel="Show title in mobile"
                                name="./showinmobileC"
                                text="Show title in mobile"/>
                            <linktitleC
                                jcr:primaryType="nt:unstructured"
                                anchor="95%"
                                defaultAlt="*"
                                defaultPath="*"
                                defaultTitle="*"
                                id="linktitleC"
                                name="./linktitleC"
                                pathAllowBlank="{Boolean}false"
                                textAllowBlank="{Boolean}false"
                                titleAllowBlank="{Boolean}false"
                                xtype="latamlink"/>
                            <fields/>
                        </items>
                    </column>
                </items>
            </thirdColum>
            <fourthColum
                jcr:primaryType="nt:unstructured"
                jcr:title="Fourth Column"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columnTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./titleColumnD"/>
                            <showtitleinmobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldLabel="Show title in mobile"
                                name="./showinmobileD"
                                text="Show title in mobile"/>
                            <linktitleD
                                jcr:primaryType="nt:unstructured"
                                anchor="95%"
                                defaultAlt="*"
                                defaultPath="*"
                                defaultTitle="*"
                                id="linktitleD"
                                name="./linktitleD"
                                pathAllowBlank="{Boolean}false"
                                textAllowBlank="{Boolean}false"
                                titleAllowBlank="{Boolean}false"
                                xtype="latamlink"/>
                            <fields/>
                        </items>
                    </column>
                </items>
            </fourthColum>
        </items>
    </content>
</jcr:root>
